<Application x:Class="PartageDepense.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PartageDepense"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <materialDesign:BundledTheme BaseTheme="Inherit" 
                                     PrimaryColor="Brown" 
                                     SecondaryColor="Yellow" />

                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesign3.Defaults.xaml" />

            </ResourceDictionary.MergedDictionaries>
            <BitmapImage x:Key="AppIcon" UriSource="https://i.ibb.co/DP5MkR57/logo-de-la-soci-t-payorelse-en-png.jpg"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>